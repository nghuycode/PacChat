<Window x:Class="PacChat.MediaPlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ccMedia="clr-namespace:PacChat.Resources.CustomControls.Media"
        xmlns:local="clr-namespace:PacChat"
        mc:Ignorable="d"
        Title="MediaPlayerWindow" Height="450" Width="800" WindowStyle="None" Background="{x:Null}">
    <Grid>
        <ccMedia:MediaPlayer x:Name="MediaPlayer"></ccMedia:MediaPlayer>
    </Grid>
</Window>
